   "outputs": [],
   "source": [
    "my_list = [\"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\"]\n",
    "\n",
    "# Version 1\n",
    "for day in my_list:\n",
    "  print(\"Today is {}\".format(day))\n",
    "    \n",
    "# Version 2\n",
    "for i in range(len(my_list)):\n",
    "  print(\"Today is again {}\".format(my_list[i]))\n"
   ]
